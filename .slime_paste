(defun contacts ()
    (loop with result = (make-hash-table :test #'equal)
          for g across (groupme-get :path '("groups"))
          do
          (loop for m in (gethash "members" g)
                do
                (adjoin-entity (gethash "nickname" m)
                               (gethash "user_id" m)
                                result))

          finally
          (return
            (ids-names result))))
